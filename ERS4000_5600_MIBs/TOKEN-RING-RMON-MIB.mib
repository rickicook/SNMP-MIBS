              rmon, statistics, history
                                          FROM RMON-MIB;

          --  Token Ring Remote Network Monitoring MIB

tokenRing MODULE-IDENTITY
    LAST-UPDATED    "200504010000Z"
    ORGANIZATION    ""
    CONTACT-INFO    ""
    DESCRIPTION     ""
    REVISION "200504010000Z" -- 31 March 2005
