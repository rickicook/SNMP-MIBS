	::= { rcChasFanTable 1 }

RcChasFanEntry ::=
	SEQUENCE {
		rcChasFanId Integer32,
		rcChasFanOperStatus INTEGER,
		rcChasFanAmbientTemperature Gauge32,
                rcChasFanType INTEGER,
 	        rcChasFanFlowType INTEGER
	}

