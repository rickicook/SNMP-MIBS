        DEFVAL        { 1025 }
        ::= { rcSyslogHostEntry  23 }
    
rcSyslogHostSecureForwardingMode   OBJECT-TYPE
        SYNTAX        INTEGER {
	                 none(1),
			 ssh(2),
			 tls(3)
		      }
        MAX-ACCESS    read-write
        STATUS        current
	DESCRIPTION   "Set Secure forwarding mode for a particular Syslog host. This object is
	              used to obsolete the rcSyslogHostSecureForwardingEnable and
		      rcSyslogHostSecureForwardingSshProtect.
		      ssh  - Set Secure forwarding mode for particular Syslog host over SSH tunnel 
		      tls  -  Set Secure forwarding mode for particular Syslog host over TLS 
		      none - Set Secure forwarding mode none for particular Syslog host"
        DEFVAL        { none }
        ::= { rcSyslogHostEntry  24 }

rcSyslogHostSecureForwardingServerCertName   OBJECT-TYPE
        SYNTAX        DisplayString (SIZE (0..64))
        MAX-ACCESS    read-write
        STATUS        current
	DESCRIPTION   "Specifies the identity name of server certificate. It is used to match
	    	       the subject name of the server certificate received during negotiation
		       with this configured server cert name to allow negotiation"
        ::= { rcSyslogHostEntry  25 }
    
