        DEFVAL        { true }
        ::= { rcSyslogHostEntry  21 }

rcSyslogHostSecureForwardingSshProtect OBJECT-TYPE
        SYNTAX        TruthValue
        MAX-ACCESS    read-write
        STATUS        deprecated
        DESCRIPTION   "Enable/Disable Secure forwarding of Syslog over SSH remote port forwarding
	               for particular host.
	               deprecate/Obsolete by rcSyslogHostSecureForwardingMode "
        DEFVAL        { true }
        ::= { rcSyslogHostEntry  22 }

rcSyslogHostSecureForwardingTcpPort OBJECT-TYPE
        SYNTAX        Integer32(1025..49151)
