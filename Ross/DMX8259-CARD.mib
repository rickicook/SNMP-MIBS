--
--  Table of Product parameters
--
dmx8259ProductTable     OBJECT-TYPE
  SYNTAX      SEQUENCE OF Dmx8259ProductEntry
  MAX-ACCESS  not-accessible
  STATUS      current
  DESCRIPTION
    "A table containing product info for all DMX8259 cards at this node.
    This table is the expansion of the table 'openGearCardTable' in the
    OPEN-GEAR-CARD-MIB. For each DMX8259 card in the 'openGearCardTable',
    there is a corresponding entry in this table, having the same indices -
    'openGearFrameIndex' and 'openGearCardIndex'."
  ::= { dmx8259CardObjects 2 }

dmx8259ProductEntry     OBJECT-TYPE
  SYNTAX      Dmx8259ProductEntry
  MAX-ACCESS  not-accessible
  STATUS      current
  DESCRIPTION
    "A conceptual row within the 'dmx8259ProductTable'."
  INDEX    { openGearFrameIndex, openGearCardIndex }
  ::= { dmx8259ProductTable 1 }

