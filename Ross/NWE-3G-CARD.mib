    FROM SNMPv2-TC;

nwe3gCardMib MODULE-IDENTITY
  LAST-UPDATED "201303011200Z"
  ORGANIZATION "www.rossvideo.org"
  CONTACT-INFO
    "postal:   Ross Video
               8 John St. P.O. Box 220
               Iroquois ON K0E 1K0

      email:   support@rossvideo.com"

  DESCRIPTION
    "Defines control and monitoring structures for the NWE3G card.
     (Add more) ..."

  REVISION     "201303011200Z"
  DESCRIPTION
