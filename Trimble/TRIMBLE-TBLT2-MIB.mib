tblt2CfgIfEthIpv4Entry OBJECT-TYPE
    SYNTAX        Tblt2CfgIfEthIpv4Entry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "IPv4 configuration entry for an ethernet port."
    INDEX
        { tblt2CfgIfEthIndex }
    ::= { tblt2CfgIfEthIpv4Table 1 }

Tblt2CfgIfEthIpv4Entry ::= SEQUENCE
{
    tblt2CfgIfEthIndex         Integer32,
    tblt2CfgIfEthIpv4Prov      Tblt2EthProv,
    tblt2CfgIfEthIpv4Addr      DisplayString,
    tblt2CfgIfEthIpv4Mask      DisplayString,
    tblt2CfgIfEthIpv4Broadcast DisplayString,
    tblt2CfgIfEthIpv4Gateway   DisplayString,
    tblt2CfgIfEthSynce         Tblt2EthSynce,
    tblt2CfgIfEthBond          Integer32,
    tblt2CfgIfEthVpriority     Integer32,
    tblt2CfgIfEthVids          DisplayString,
    tblt2CfgIfEthIpv6Prov      Tblt2EthProv,
    tblt2CfgIfEthIpv6Addr      DisplayString,
    tblt2CfgIfEthAutoNeg       Integer32
}

tblt2CfgIfEthIndex OBJECT-TYPE
    SYNTAX        Integer32 (1..3)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
          "Logical index of ethernet ports (1=eth0, 2=eth1, 3=eth2)"
    ::= { tblt2CfgIfEthIpv4Entry 1 }

tblt2CfgIfEthIpv4Prov OBJECT-TYPE
    SYNTAX        Tblt2EthProv
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Controls the provisioning type for the Management Ethernet Port.
         A setting of 1 disables the port.
         A setting of 2 enables static provisioning.
         A setting of 3 enables DHCP provisioning."
    ::= { tblt2CfgIfEthIpv4Entry 2 }

tblt2CfgIfEthIpv4Addr OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Internet address for ethernet port, e.g. 192.168.0.1
         If this field is a single hyphen, DHCP is in use."
    ::= { tblt2CfgIfEthIpv4Entry 3 }

tblt2CfgIfEthIpv4Mask OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "IPv4 netmask, e.g. 255.255.254.0"
    ::= { tblt2CfgIfEthIpv4Entry 4 }

tblt2CfgIfEthIpv4Broadcast OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "IPv4 broadcast, e.g. 192.168.1.255"
    ::= { tblt2CfgIfEthIpv4Entry 5 }

tblt2CfgIfEthIpv4Gateway OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "IPv4 gateway address e.g. 192.168.1.100"
    ::= { tblt2CfgIfEthIpv4Entry 6 }

tblt2CfgIfEthSynce OBJECT-TYPE
    SYNTAX        Tblt2EthSynce
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "SyncE configuration.  Only available for eth0 and eth1.
         Valid values are 1 (off), 2 (output), 3 (input)."
    ::= { tblt2CfgIfEthIpv4Entry 7 }

tblt2CfgIfEthBond OBJECT-TYPE
    SYNTAX        Integer32 (1..3)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Port bonding configuration.  Only available for eth0 and eth1.
         Valid values are 1 (disable), 2 (enable), 3 (standby)."
    ::= { tblt2CfgIfEthIpv4Entry 8 }

tblt2CfgIfEthVpriority OBJECT-TYPE
    SYNTAX        Integer32 (0..7)
    MAX-ACCESS    read-write
    STATUS        deprecated
    DESCRIPTION
        "VLAN priority configuration.  Only available for eth0 and eth1.
         Valid values are (0..7). VLAN IDs must be set before configuring
         VLAN priority. Time protocols must be disabled to change this value."
    ::= { tblt2CfgIfEthIpv4Entry 9 }

tblt2CfgIfEthVids OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "VLAN IDs.  Only available for eth0 and eth1.
         This is a comma separated list of id numbers,
         each in range 3..4094.
         There can be a maximum of eight ids assigned.
         Writing the special string '-1' will disable
         VLAN configuration, and remove all vlan settings.
         NOTE: vlan IDs, and all VlanTable parameters cannot
         be set while any timing protocol is enabled on the interface.
         Requests to modify these values while a timing
         protocol is active will be rejected with
         an 'inconsistent value' error."
    ::= { tblt2CfgIfEthIpv4Entry 10 }

tblt2CfgIfEthIpv6Prov OBJECT-TYPE
    SYNTAX        Tblt2EthProv
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Controls the IPv6 provisioning type for the Ethernet Port.
         A setting of 1 disables IPv6.
         A setting of 2 enables static provisioning.
         A setting of 3 enables DHCP provisioning."
    ::= { tblt2CfgIfEthIpv4Entry 11 }

tblt2CfgIfEthIpv6Addr OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Internet v6 address for ethernet port, 
         e.g. fd6b:fd64:9e0c:beef::/128
         If this field is a single hyphen, 
         default address is assigned."
    ::= { tblt2CfgIfEthIpv4Entry 12 }

tblt2CfgIfEthAutoNeg OBJECT-TYPE
    SYNTAX        Integer32 (0..1)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Port auto-negotiation configuration.  
         Only available for eth0 and eth1.
         Valid values are 0 (disable), 1 (enable)."
    ::= { tblt2CfgIfEthIpv4Entry 13 }


tblt2CfgIfEthVlanTable OBJECT-TYPE
