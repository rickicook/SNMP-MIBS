tblt2AlTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF Tblt2AlEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The alarms table."
    ::= { tblt2Alarms 1 }

tblt2AlEntry OBJECT-TYPE
    SYNTAX        Tblt2AlEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "Row describing alarm information."
    INDEX
        { tblt2AlIndex }
    ::= { tblt2AlTable 1 }

Tblt2AlEntry ::= SEQUENCE
{
    tblt2AlIndex     Integer32,
    tblt2AlNumber    Integer32,
    tblt2AlName      DisplayString,
    tblt2AlLevel     Tblt2AlarmLevel,
    tblt2AlActive    Integer32,
    tblt2AlIsSet     Integer32,
    tblt2AlSetTime   Integer32,
    tblt2AlClrTime   Integer32,
    tblt2AlStateTime Integer32
}


tblt2AlIndex OBJECT-TYPE
    SYNTAX        Integer32 (1..32)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Alarm entry index (1 based)"
    ::= { tblt2AlEntry 1 }

tblt2AlNumber OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Number for the alarm. (0 based)"
    ::= { tblt2AlEntry 2 }

tblt2AlName OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Name of the alarm."
    ::= { tblt2AlEntry 3 }

tblt2AlLevel OBJECT-TYPE
    SYNTAX        Tblt2AlarmLevel
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Severity level of the alarm. May be one of
         Ignored(0), Notification(1), Minor(2), Major(4), Critical(8),
         or Disabled(64)"
    ::= { tblt2AlEntry 4 }

tblt2AlActive OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Whether this alarm is currently in an 'alarmed' state (1), or
         is not (0)."
    ::= { tblt2AlEntry 5 }

tblt2AlIsSet OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "If value 1, indicates that the underlying condition is true.
         If the Alarm Set Time has not expired or the Alarm Level is
         set to Ignored, this value may differ from tblt2AlActive."
    ::= { tblt2AlEntry 6 }

tblt2AlSetTime OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Alarm Set Time is the number of seconds the underlying condition must
         be true before the alarm is made 'Active'."
    ::= { tblt2AlEntry 7 }

tblt2AlClrTime OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Alarm Clear Time is the number of seconds the underlying condition must
         be calse before the alarm is cleared."
    ::= { tblt2AlEntry 8 }

tblt2AlStateTime OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Alarm State Time is the number of seconds the underlying condition has
         been steady."
    ::= { tblt2AlEntry 9 }

