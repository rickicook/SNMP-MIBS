tblt2CfgIfEthVlanVid OBJECT-TYPE
    SYNTAX        Integer32 (3..4094)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "VLAN VID, as set in tblt2CfgIfEthVids. Range is 3 to 4094."
    ::= { tblt2CfgIfEthVlanEntry 2 }

tblt2CfgIfEthVlanStatus OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Connection status string for this VLAN entry."
    ::= { tblt2CfgIfEthVlanEntry 3 }

tblt2CfgIfEthVlanProv OBJECT-TYPE
    SYNTAX        Tblt2EthProv
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Type of address provisioning for this VLAN entry. One of:
         provDisabled(1), provStatic(2), or provDhcp(3)."
    ::= { tblt2CfgIfEthVlanEntry 4 }

tblt2CfgIfEthVlanAddr OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Ipv4 address for this VLAN entry."
    ::= { tblt2CfgIfEthVlanEntry 5 }

tblt2CfgIfEthVlanMask OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "IPv4 netmask for this VLAN entry."
    ::= { tblt2CfgIfEthVlanEntry 6 }

tblt2CfgIfEthVlanBroadcast OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "IPv4 broadcast address for this VLAN entry."
    ::= { tblt2CfgIfEthVlanEntry 7 }

tblt2CfgIfEthVlanGateway OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "IPv4 gateway address for this VLAN entry."
    ::= { tblt2CfgIfEthVlanEntry 8 }

tblt2CfgIfEthVlanIpv6Link OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "IPv6 Link address for this VLAN entry."
    ::= { tblt2CfgIfEthVlanEntry 9 }

tblt2CfgIfEthVlanPrio OBJECT-TYPE
    SYNTAX        Integer32 (0..7)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "VLAN priority configuration. Valid values are (0..7), default is 0.
         Time protocols must be disabled to change this value."
    ::= { tblt2CfgIfEthVlanEntry 10 }

tblt2CfgIfSnmpTrap OBJECT IDENTIFIER ::= { tblt2CfgInterfaces 3 }

