Tblt2AlarmActivity ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to show the whether an alarm is currently in an 'alarmed'
         state (Active), or is currently off (Cleared)"
    SYNTAX        INTEGER {
        alarmCleared(1),
        alarmActive(2)
    }

Tblt2AlarmLevel ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate (and set) the severity of an alarm. One of:
         Ignored(0), Notification(1), Minor(2), Major(4), Critical(8),
         or Disabled(64)."
    SYNTAX        INTEGER {
        alarmIgnored(0),
        alarmNotification(1),
        alarmMinor(2),
        alarmMajor(4),
        alarmCritical(8),
        alarmDisabled(64)
    }

Tblt2EthProv ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the type of provisioning for an ethernet
         port.  One of: provDisabled(1), provStatic(2), or provDhcp(3)"
    SYNTAX        INTEGER {
        provDisabled(1),
        provStatic(2),
        provDhcp(3)
    }

Tblt2EthSynce ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the SyncE configuration for an ethernet
         port.  One of: synceOff(1), synceOutput(2), synceInput(3)."
    SYNTAX        INTEGER {
        synceOff(1),
        synceOutput(2),
        synceInput(3)
    }

Tblt2PtpStatus ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the PTP status for an ethernet port.
         One of ptpDisable(1), ptpEnable(2), ptpDefault(3)."
    SYNTAX        INTEGER {
        ptpDisable(1),
        ptpEnable(2),
        ptpDefault(3)
    }

Tblt2PtpProfile ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the PTP profile for an ethernet port."
    SYNTAX        INTEGER {
        ptpG8275-1(1),
        ptpG8265-1-II(2),
        ptpIeee1588(3),
        ptpTelecom(4),
        ptpPower(5),
        ptpG8275-2(6),
        ptpEnterprise(7),
        ptpG8265-1-I(8),
        ptpSMPTE(9)
    }

Tblt2PtpSyncMode ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the PTP Sync Mode for an ethernet port."
    SYNTAX        INTEGER {
        ptpOneStep(1),
        ptpTwoStep(2)
    }

Tblt2PtpTransport ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the PTP Transport Mode for an ethernet port."
    SYNTAX        INTEGER {
        ptpTransIpv4(1),
        ptpTransIpv6(2),
        ptpTransIeee8023(3)
    }

Tblt2PtpIpMode ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the PTP IP mode (Multicast, Unicast or Hybrid)."
    SYNTAX        INTEGER {
        ptpIpMulticast(1),
        ptpIpUnicast(2),
        ptpIpHybrid(3)
    }

Tblt2PtpDelayMech ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the PTP Delay Mechanism (E2E or P2P)."
    SYNTAX        INTEGER {
        ptpDelayDisabled(0),
        ptpE2E(1),
        ptpP2P(2)
    }

Tblt2PtpL2Mac ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate PTP 803.2 Multicast MAC in use, 
         def(1): 01-1B-19-00-00-00 (routable)
         alt(2): 01-80-C2-00-00-0E (non-routable)"
    SYNTAX        INTEGER {
        def(1),
        alt(2)
    }

Tblt2PtpOperMode ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate the PTP Operational Mode for an ethernet port."
    SYNTAX        INTEGER {
        normal(0),
        freerun(1)
    }

Tblt2PtpMode ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate master or slave mode for
        a PTP clock."
    SYNTAX        INTEGER {
        ptpMaster(1),
        ptpSlave(2)
    }

Tblt2DisEn ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate a property that is disabled/enabled."
    SYNTAX        INTEGER {
        disable(1),
        enable(2)
    }

Tblt2EnDis ::= TEXTUAL-CONVENTION
    STATUS        current
    DESCRIPTION
        "Used to indicate a property that is enabled/disabled."
    SYNTAX        INTEGER {
