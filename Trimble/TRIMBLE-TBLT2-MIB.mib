    SYNTAX        SEQUENCE OF Tblt2CfgIfSnmpTrapEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "Table of configuration entries for SNMP trap destinations."
    ::= { tblt2CfgIfSnmpTrap 1 }

tblt2CfgIfSnmpTrapEntry OBJECT-TYPE
    SYNTAX        Tblt2CfgIfSnmpTrapEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "SNMP trap destination configuration entry."
    INDEX
        { tblt2CfgIfSnmpTrapIndex }
    ::= { tblt2CfgIfSnmpTrapTable 1 }

Tblt2CfgIfSnmpTrapEntry ::= SEQUENCE
{
    tblt2CfgIfSnmpTrapIndex  Integer32,
    tblt2CfgIfSnmpTrapEnable Integer32,
    tblt2CfgIfSnmpTrapHost   DisplayString,
    tblt2CfgIfSnmpTrapPort   Integer32,
    tblt2CfgIfSnmpTrapType   Integer32,
    tblt2CfgIfSnmpTrapComm   DisplayString,
    tblt2CfgIfSnmpTrapName   DisplayString,
    tblt2CfgIfSnmpTrapPass   DisplayString,
    tblt2CfgIfSnmpTrapPriv   DisplayString
}

tblt2CfgIfSnmpTrapIndex OBJECT-TYPE
    SYNTAX        Integer32 (1..4)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Index of SNMP trap destination entry"
    ::= { tblt2CfgIfSnmpTrapEntry 1 }

tblt2CfgIfSnmpTrapEnable OBJECT-TYPE
    SYNTAX        Integer32 (0..1)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Disable (0) or enable (1) this entry"
    ::= { tblt2CfgIfSnmpTrapEntry 2 }

tblt2CfgIfSnmpTrapHost OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
