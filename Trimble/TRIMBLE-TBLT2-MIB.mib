tblt2CfgOutputs OBJECT IDENTIFIER ::= { tblt2Configuration 4 }

tblt2CfgOutBnc OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Controls the status of the BNC output. Possible values are:
         Off(1), Low(2), High(3), PPS(4), Even(5), 10MHz(6), Periodic(7),
         Invert-Off(-1), Invert-Low(-2), Invert-High(-3), Invert-PPS(-4),
         Invert-Even(-5), Invert-10Hz(-6), Invert-Periodic(-7)."
    ::= { tblt2CfgOutputs 1 }

tblt2CfgOutOutWidth OBJECT-TYPE
    SYNTAX        Integer32 (100..500000000)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Width, in nanoseconds, of the output pulse.
         Valid range is 100 to 500000000 (5E8)."
    ::= { tblt2CfgOutputs 2 }

tblt2CfgOutOutDelay OBJECT-TYPE
    SYNTAX        Integer32 (-500000000..500000000)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Delay, in nanoseconds, of the output pulse.
         This is used to compensate for cable and other delays.
         Valid range is -5E8 to 5E8."
    ::= { tblt2CfgOutputs 3 }

tblt2CfgOutPerWidth OBJECT-TYPE
    SYNTAX        Integer32 (100..500000000)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Width, in nanoseconds, of the periodic output system.
         Valid range is 100 to 5E8."
    ::= { tblt2CfgOutputs 4 }

tblt2CfgOutPerPeriod OBJECT-TYPE
    SYNTAX        Integer32 (2..100000)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "This sets the time modulo value (period) for the periodic
         output.  Valid range is 2..100000. This allows for complex
         periodic outputs. To achieve an output of once per second,
         set tblt2CfgOutBnc to PPS(4)."
    ::= { tblt2CfgOutputs 5 }

tblt2CfgOutPerValue OBJECT-TYPE
    SYNTAX        Integer32 (0..100000)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Periodic comparison value. Valid range is 0..100000.
         This value is used to determine time alignment of the
         periodic output, in seconds. This value is compared with
         the modulo time based on the tblt2CfgOutPerPeriod value.
         This allows for complex periodic pulses to be generated.
         Note that it is possible to disable the periodic output
         by setting this to a value that will never be satisfied
         based on the modulo setting."
    ::= { tblt2CfgOutputs 6 }

tblt2CfgPtp OBJECT IDENTIFIER ::= { tblt2Configuration 5 }

tblt2CfgPtpTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF Tblt2CfgPtpEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "Table of ethernet port PTP configuration entries."
    ::= { tblt2CfgPtp 1 }

tblt2CfgPtpEntry OBJECT-TYPE
    SYNTAX        Tblt2CfgPtpEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "Row describing ethernet port PTP configuration."
    INDEX
        { tblt2CfgPtpIndex }
    ::= { tblt2CfgPtpTable 1 }

Tblt2CfgPtpEntry ::= SEQUENCE
{
    tblt2CfgPtpIndex Integer32,
    tblt2CfgPtpStatus Tblt2PtpStatus,
    tblt2CfgPtpProfile Tblt2PtpProfile,
    tblt2CfgPtpSyncMode Tblt2PtpSyncMode,
    tblt2CfgPtpTransp Tblt2PtpTransport,
    tblt2CfgPtpIpMode Tblt2PtpIpMode,
    tblt2CfgPtpDelay Tblt2PtpDelayMech,
    tblt2CfgPtpDomain Integer32,
    tblt2CfgPtpAnnInter Integer32,
    tblt2CfgPtpAnnTimeout Integer32,
    tblt2CfgPtpSyncInter Integer32,
    tblt2CfgPtpReqInter Integer32,
    tblt2CfgPtpClockID DisplayString,
    tblt2CfgPtpPri1 Integer32,
    tblt2CfgPtpPri2 Integer32,
    tblt2CfgPtpClass Integer32,
    tblt2CfgPtpHoldTime Integer32,
    tblt2CfgPtpPDelayReqInter Integer32,
    tblt2CfgPtpDuration Integer32,
    tblt2CfgPtpGrantor DisplayString,
    tblt2CfgPtpTtl Integer32,
    tblt2CfgPtpL2Mac Tblt2PtpL2Mac,
    tblt2CfgPtpOperMode Tblt2PtpOperMode,
    tblt2CfgPtpMode Tblt2PtpMode
    }

tblt2CfgPtpIndex OBJECT-TYPE
    SYNTAX        Integer32 (1..2)
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Index in table, corresponding to the ethernet port.
         1 is eth0, 2 is eth1."
    ::= { tblt2CfgPtpEntry 1 }

tblt2CfgPtpStatus OBJECT-TYPE
    SYNTAX        Tblt2PtpStatus
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Disable/Enable/Default.  Possible values are
         disable(1), enable(2), default(3).  This instance
         must be disabled before the values in this row
         may be set.  Setting this to default will reset
         all values in the row to defaults, based on currently
         selected profile."
    ::= { tblt2CfgPtpEntry 2 }

tblt2CfgPtpProfile OBJECT-TYPE
    SYNTAX        Tblt2PtpProfile
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "PTP profile.  Possible values are ptpG8275(1), ptpG8265(2)
         ptpIeee1588(3), ptpTelecom(4), ptpPower(5)ptpG8275.2(6), 
         ptpEnterprise(7), ptpG8265.1.I(8), ptpSMPTE(9)."
    ::= { tblt2CfgPtpEntry 3 }
        

tblt2CfgPtpSyncMode OBJECT-TYPE
    SYNTAX        Tblt2PtpSyncMode
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Sync-Mode.  Possible values are ptpOneStep(1) and
         ptpTwoStep(2)."
    ::= { tblt2CfgPtpEntry 4 }

tblt2CfgPtpTransp OBJECT-TYPE
    SYNTAX        Tblt2PtpTransport
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Transport Protocol.  Possible values are
         IPv4(1), IPv6(2), IEEE 802.3(3)."
    ::= { tblt2CfgPtpEntry 5 }

tblt2CfgPtpIpMode OBJECT-TYPE
    SYNTAX        Tblt2PtpIpMode
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "IP Mode.  Possible values are Multicast(1),
         Unicast(2), Hybrid(3)."
    ::= { tblt2CfgPtpEntry 6 }

tblt2CfgPtpDelay OBJECT-TYPE
    SYNTAX        Tblt2PtpDelayMech
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Delay Mechanism.  Possible values are
         E2E(1) or P2P(2)."
    ::= { tblt2CfgPtpEntry 7 }

tblt2CfgPtpDomain OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Domain number.  Valid range is [0-255]."
    ::= { tblt2CfgPtpEntry 8 }

tblt2CfgPtpAnnInter OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Announce Interval.  Valid range is [-7..7]."
    ::= { tblt2CfgPtpEntry 9 }

tblt2CfgPtpAnnTimeout OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Announce Timeout. Valid range is [2-255]."
    ::= { tblt2CfgPtpEntry 10 }

tblt2CfgPtpSyncInter OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Sync Interval. Valid range is [-7..7]."
    ::= { tblt2CfgPtpEntry 11 }

tblt2CfgPtpReqInter OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Delay Request Interval. Valid range is [-7..7]."
    ::= { tblt2CfgPtpEntry 12 }

tblt2CfgPtpClockID OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "String displaying the PTP Clock ID for this port. If the
         port is disabled, this string will be empty."
    ::= { tblt2CfgPtpEntry 13 }

tblt2CfgPtpPri1 OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Priority 1. Valid range is [0-255]."
    ::= { tblt2CfgPtpEntry 14 }

tblt2CfgPtpPri2 OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Priority 2. Valid range is [0-255]."
    ::= { tblt2CfgPtpEntry 15 }

tblt2CfgPtpClass OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Clock class. Valid range is [0-254]."
    ::= { tblt2CfgPtpEntry 16 }

tblt2CfgPtpHoldTime OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Holdover time in minutes. Valid range is [0-2880]. 
        75 minutes is the default, 0=disable.
        This is a 111224-35 variant option only."
    ::= { tblt2CfgPtpEntry 17 }

tblt2CfgPtpPDelayReqInter OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Peer Delay Request Interval. Valid range is [-7..7]."
    ::= { tblt2CfgPtpEntry 18 }

tblt2CfgPtpDuration OBJECT-TYPE
    SYNTAX        Integer32 (10..1000)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Duration field for unicast grant messages. Valid range
         is 10-1000, 300 is typical default."
    ::= { tblt2CfgPtpEntry 19 }

tblt2CfgPtpGrantor OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "For unicast input profiles only. This allows setting the
         unicast GrandMaster to use as the grantor for requests.
         Format is a single IPv4 or IPv6 address.
         A special value of - clears the field.
         Setting this value when not using a unicast input
         profile will result in an inconsistentValue error."
    ::= { tblt2CfgPtpEntry 20 }

tblt2CfgPtpTtl OBJECT-TYPE
    SYNTAX        Integer32 (1..255)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Set the multicast ttl value for the transmission.
         Valid range is 1-255, although some profiles restrict
         this range to 1-6 or less."
    ::= { tblt2CfgPtpEntry 21 }

tblt2CfgPtpL2Mac OBJECT-TYPE
    SYNTAX        Tblt2PtpL2Mac 
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Set the 803.2 Multicast MAC. 
         Valid values are default(1) and alternate(2):
         def(1): 01-1B-19-00-00-00 (routable)
         alt(2): 01-80-C2-00-00-0E (non-routable)
         Profiles that allow this setting to be modified
         include: 8275.1, 1588, and Power."
    DEFVAL { 1 }
    ::= { tblt2CfgPtpEntry 22 }

tblt2CfgPtpOperMode OBJECT-TYPE
    SYNTAX        Tblt2PtpOperMode
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "PTP Operational Mode.  
         Possible values are normal(0), freerun(1)."
    ::= { tblt2CfgPtpEntry 23 }
        
tblt2CfgPtpMode OBJECT-TYPE
    SYNTAX        Tblt2PtpMode
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Set the current clock mode. Valid values are
        ptpMaster(1) and ptpSlave(2)."
    ::= { tblt2CfgPtpEntry 24 }

tblt2CfgNtp OBJECT IDENTIFIER ::= { tblt2Configuration 6 }

tblt2CfgNtpSec OBJECT IDENTIFIER ::= { tblt2CfgNtp 1 }

tblt2CfgNtpSecEnc OBJECT-TYPE
    SYNTAX        Tblt2DisEn
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Enable/Disable NTP encryption.  Possible values are
         Disable(1), Enable(2)."
    ::= { tblt2CfgNtpSec 1 }

tblt2CfgNtpSecHost OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Host name for the encryption certificate.
         Allowed characters are A-Za-z0-9 and _."
    ::= { tblt2CfgNtpSec 2 }

tblt2CfgNtpSecGrp OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Group name for the encryption certificate.
         Allowed characters are A-Za-z0-9 and _."
    ::= { tblt2CfgNtpSec 3 }

tblt2CfgNtpSecRenew OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Allows the renewal of the encryption certificate.
         Writing a 2 to this OID will cause the certificate
         to be renewed.  This should be done approximately
         every 30 days to keep the certificate valid."
    ::= { tblt2CfgNtpSec 4 }

tblt2CfgNtpTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF Tblt2CfgNtpEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "Table of ethernet port NTP configuration entries."
    ::= { tblt2CfgNtp 2 }

tblt2CfgNtpEntry OBJECT-TYPE
    SYNTAX        Tblt2CfgNtpEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "Row describing ethernet port NTP configuration."
    INDEX
        { tblt2CfgNtpIndex }
    ::= { tblt2CfgNtpTable 1 }

Tblt2CfgNtpEntry ::= SEQUENCE
{
    tblt2CfgNtpIndex   Integer32,
    tblt2CfgNtpProto   Integer32,
    tblt2CfgNtpBcastEn Tblt2DisEn,
    tblt2CfgNtpBcastIp DisplayString,
    tblt2CfgNtpBcastInter Integer32,
    tblt2CfgNtpBcastTtl Integer32
}

tblt2CfgNtpIndex OBJECT-TYPE
    SYNTAX        Integer32 (1..2)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Index in table, corresponding to the ethernet port.
         1 is eth0, 2 is eth1."
    ::= { tblt2CfgNtpEntry 1 }

tblt2CfgNtpProto OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "NTP Protocol enable.  This allows enable, disable or
         reset of the NTP protocol.  Possible values are
         Disable(1), Enable(2), Reset(3).  Writing Reset(3) will
         reset NTP parameters to default values."
    ::= { tblt2CfgNtpEntry 2 }

tblt2CfgNtpBcastEn OBJECT-TYPE
    SYNTAX        Tblt2DisEn
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "NTP Protocol broadcast enable.  This enables or disables
         Broadcast NTP protocol. Valid settings are Disable(1), or
         Enable(2)."
    ::= { tblt2CfgNtpEntry 3 }

tblt2CfgNtpBcastIp OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "NTP Broadcast IP address. If broadcast is enabled, this
         IP must be in the same domain as the domain of the port."
    ::= { tblt2CfgNtpEntry 4 }

tblt2CfgNtpBcastInter OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "NTP Broadcast Interval, in seconds to the power of 2.
         For example, a value of 4 sets the broadcast interval
         to 2^4 = 16 seconds.  Allowed range is [4-17]."
    ::= { tblt2CfgNtpEntry 5 }

tblt2CfgNtpBcastTtl OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "NTP Broadcast packet TTL.  Allowed range is [1..7]."
    ::= { tblt2CfgNtpEntry 6 }

tblt2CfgNtpServer OBJECT IDENTIFIER ::= { tblt2CfgNtp 3 }

tblt2CfgNtpPeers OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "String of NTP Peer IP address(es), comma-delimited without spaces. 
         Enables NTP Peering when one peer address has been assigned.
         For example, '192.168.101.250,172.16.123.250'; the NTP server will
         automatically determine the appropriate interface to use for 
         peerring based on the system's interfaces and VLANs."
    ::= { tblt2CfgNtpServer 1 }

tblt2CfgGnss OBJECT IDENTIFIER ::= { tblt2Configuration 7 }

tblt2CfgGnssConst OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "String to configure the satellite constellation(s) to use.
         For example, 'gps|GLO' would enable the GPS and GLONASS
         constellations. The configuration string can be any combination
         of gps, glo, bds, gal, qzs, separated by '|'.  GLO is GLONASS,
         BDS is the Beidou constellation, GAL is the Galileo constellation,
         and QZS is the QZSS constellation.  Note that selecting QZSS also
         enables GPS."
    ::= { tblt2CfgGnss 1 }

tblt2CfgGnssMode OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "String controlling the positioning mode of the GNSS system.
         Possible values are: 'auto' (automatic mode), 'survey' (survey mode),
         'dynamic' (dynamic mode) used when position is constantly changing,
         and a string representing the approximate location of the unit
         (manual mode), in the format '<lat> <lon> <ht>' where <lat> and
         <lon> are in degrees and <ht> is in meters (HAE).
         Note that the position will be validated by the receiver for
         accuracy and, if it is too far out of range (thereby making the
         timing of the unit inaccurate) the position will be recomputed.
         'auto' sets the unit to not force a user entered position on startup.
         If the unit has a stored position then it will be used on startup,
         with the same validation criteria as used for a user-entered position.
         'survey' forces the unit to recompute a surveyed position.  The
         surveyed position will then be used by the system on the next startup
         (to speed startup).  This also forces 'auto' mode."
    ::= { tblt2CfgGnss 2 }

tblt2CfgGnssPos OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "A string describing the current latitude, longitude and altitude
         of the unit. Latitude and longitude are given in degrees, and
         altitude is in meters (HAE)"
    ::= { tblt2CfgGnss 3 }

tblt2CfgGnssElev OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Satellite elevation mask, in degrees. Valid range is [0.0-90.0)."
    ::= { tblt2CfgGnss 4 }

tblt2CfgGnssLevel OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "The aquisition/tracking signal level. Valid range is [0.0-55.0)."
    ::= { tblt2CfgGnss 5 }

tblt2CfgGnssPdop OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "PDOP mask level. Valid range is (0.0-10.0)."
    ::= { tblt2CfgGnss 6 }
 
tblt2CfgGnssAntPwr OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Enables or disables power to the antenna. If power is turned
         off, then no status will be generated and no alarm conditions
         are available (they will be cleared). Possible values are
         on(1), off(2)."
    ::= { tblt2CfgGnss 7 }
    
tblt2CfgGnssRestart OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Allows a warm or cold restart of the GNSS receiver.
         Possible values are Do Nothing(1), Warm Restart(2),
         Cold Restart (3)."
     ::= { tblt2CfgGnss 8 }

tblt2CfgGnssSurveyLength OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Survey length. This is the number of position
         fixes that will be averaged. Only fixes that match
         other criteria (PDOP) will be used in the average.
         Acceptable range is from 60 (1 minute) to
         259200 (3 days)."
    ::= { tblt2CfgGnss 9 }

tblt2CfgGnssAntennaDelay OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Antenna delay for the system. This affects
         all timing outputs from the system. <d> is in
         nanoseconds with a range of +/- 50000000 (50ms)."
    ::= { tblt2CfgGnss 10 }

tblt2CfgSource OBJECT IDENTIFIER ::= { tblt2Configuration 8 }

tblt2CfgSrcSynceEth0 OBJECT-TYPE
    SYNTAX        Tblt2DisEn
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Allows use of SyncE-eth0 as a frequency control reference
         input candidate.  Possible values are 1 (Disabled)
         or 2 (Enabled)."
    ::= { tblt2CfgSource 1 }

tblt2CfgSrcSynceEth1 OBJECT-TYPE
    SYNTAX        Tblt2DisEn
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Allows use of SyncE-eth1 as a frequency control reference
         input candidate.  Possible values are 1 (Disabled)
         or 2 (Enabled)."
    ::= { tblt2CfgSource 2 }

tblt2CfgSrcPtpEth0 OBJECT-TYPE
    SYNTAX        Tblt2DisEn
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Allows use of PTP-eth0 as a frequency control reference
         input candidate.  Possible values are 1 (Disabled)
         or 2 (Enabled)."
    ::= { tblt2CfgSource 3 }

tblt2CfgSrcPtpEth1 OBJECT-TYPE
    SYNTAX        Tblt2DisEn
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Allows use of PTP-eth1 as a frequency control reference
         input candidate.  Possible values are 1 (Disabled)
         or 2 (Enabled)."
    ::= { tblt2CfgSource 4 }

tblt2Events OBJECT IDENTIFIER ::= { trimbleTBlt2 4 }

tblt2EvNotifications OBJECT IDENTIFIER ::= { tblt2Events 1 }

tblt2EvNfyPrefix OBJECT IDENTIFIER ::= { tblt2EvNotifications 1 }

tblt2EvNfyAlarm NOTIFICATION-TYPE
    OBJECTS
{
    tblt2EvNfyAlNumber,
    tblt2EvNfyAlDescr,
    tblt2EvNfyAlLevel,
    tblt2EvNfyAlActive,
    tblt2EvNfyAlActList,
    tblt2EvNfyAlSerNum
}
    STATUS        current
    DESCRIPTION
        "Notification for change of alarm state."
    ::= { tblt2EvNfyPrefix 1 }

tblt2EvNfyObject OBJECT IDENTIFIER ::= { tblt2EvNotifications 2 }

tblt2EvNfyAlNumber OBJECT-TYPE
    SYNTAX      Integer32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Alarm number (zero-based) of the relevant alarm."
    ::= { tblt2EvNfyObject 1 }

tblt2EvNfyAlDescr OBJECT-TYPE
    SYNTAX        DisplayString
    MAX-ACCESS    accessible-for-notify
    STATUS        current
    DESCRIPTION
        "Description of alarm, including set/clear, name, number and severity."
    ::= { tblt2EvNfyObject 2 }

tblt2EvNfyAlLevel OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    accessible-for-notify
    STATUS        current
    DESCRIPTION
        "Severity level of the alarm. May be one of
         IGN(1), NFY(2), MIN(4), MAJ(8), or CRI(16)."
    ::= { tblt2EvNfyObject 3 }

tblt2EvNfyAlActive OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    accessible-for-notify
    STATUS        current
    DESCRIPTION
        "Whether this alarm is currently in an 'alarmed' state (2), or
         is not (1)."
    ::= { tblt2EvNfyObject 4 }

tblt2EvNfyAlActList OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    accessible-for-notify
    STATUS        current
    DESCRIPTION
        "This is a bit-field representing the currently-active state of
         all system alarms. The presence of a 1 indicates that the
         corresponding alarm is active."
    ::= { tblt2EvNfyObject 5 }

tblt2EvNfyAlSerNum OBJECT-TYPE
    SYNTAX        Integer32
    MAX-ACCESS    accessible-for-notify
    STATUS        current
    DESCRIPTION
        "This is the serial number of the unit producing this trap."
    ::= { tblt2EvNfyObject 6 }

tblt2Conformance OBJECT IDENTIFIER ::= { trimbleTBlt2 5 }

tblt2ConfGroups OBJECT IDENTIFIER ::= { tblt2Conformance 1 }

